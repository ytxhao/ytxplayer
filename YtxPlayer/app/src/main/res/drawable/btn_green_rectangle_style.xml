<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false" android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/button_green_style_normal_state_color_default"/>
            <corners android:radius="5dp"/>
            <stroke android:color="#32000000" android:width="1px"/>
        </shape>
    </item>
    <item android:state_pressed="true" android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/button_green_style_pressed_state_color_default"/>
            <corners android:radius="5dp"/>
            <stroke android:color="#32000000" android:width="1px"/>
        </shape>
    </item>

    <item  android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/button_green_style_disable_state_color_default"/>
            <corners android:radius="5dp"/>
            <stroke android:color="#32000000" android:width="1px"/>
        </shape>
    </item>
</selector>